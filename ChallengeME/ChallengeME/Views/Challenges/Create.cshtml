@model Challenge
@{
    Layout = "_myLayout";

}


<h1> CREATE CHALLENGE VIEW </h1>



<form asp-action="create" method="post" class="form-horizontal" autocomplete="off" asp-route-id="">
    <div class="form-group">
        <label asp-for="Title">Title: </label>
        <div>
            <input asp-for="Title" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Description">Description: </label>
        <div>
            <input asp-for="Description" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Difficulty">Difficulty: </label>
        <div>
            <input asp-for="Difficulty" />
        </div>
    </div>


    <div class="form-group">
        <input type="submit" class="btn btn-primary" value="SUBMIT" />
    </div>
</form>


